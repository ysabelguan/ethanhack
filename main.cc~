#include "smartarray.h" //do not include source file 

// MAIN FUNCTION

int main()
{
  //  
  try
    {
      SmartArray array1(3);
      SmartArray array1(int size);
      double appendage = 34;
      int indexvalue = 3;
      double newvalue = 69; 
      array1.append(appendage);
      array1.remove(indexvalue);
      array1.setValue(indexvalue, newvalue);
      array1.getValue(indexvalue);
    }
